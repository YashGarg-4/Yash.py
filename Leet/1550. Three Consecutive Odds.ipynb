   "source": [
    "class Solution:\n",
    "    def threeConsecutiveOdds(self, arr: List[int]) -> bool:\n",
    "        if len(arr)<3:\n",
    "            return False\n",
    "        for i in range(len(arr)-2):\n",
    "            if arr[i]%2==1 and arr[i+1]%2==1 and arr[i+2]%2 ==1:\n",
    "                return True\n",
    "        return False"
   ]
  }
