   "source": [
    "class Solution:\n",
    "    def minimumOperations(self, nums: List[int]) -> int:\n",
    "        ans = 0\n",
    "        for i in nums:\n",
    "            if i%3 !=0:\n",
    "                ans+=1\n",
    "        return ans"
   ]
