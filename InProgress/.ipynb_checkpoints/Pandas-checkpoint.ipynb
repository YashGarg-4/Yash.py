   "source": [
    "#Virginica petal lengths greater than 1.5\n",
    "import pandas as pd\n",
    "flowers = pd.read_csv('iris.csv')\n",
    "df = flowers.copy()\n",
    "df.rename(columns = {'petal.length':'pl'}, inplace = True)\n",
    "a = df[df.variety == 'Virginica']\n",
    "a = a[a.pl > 1.5]\n",
    "a.describe()"
   ]
  }
